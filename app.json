{
  "expo": {
    "name": "MyTasks",
    "slug": "MyTasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "mytasks.demo",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./src/images/logo.png"
    }
  }
}
